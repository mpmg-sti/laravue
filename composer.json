{
    "name": "mpmg/laravue",
    "description": "Code generation for MPMG projects that use Laravel and VueJs tecnologies.",
    "type": "library",
    "keywords": [
        "mpmg",
        "vue",
        "vuejs",
        "laravel"
    ],
    "homepage": "https://github.com/mpmg-sti/laravue",
    "license": "MIT",
    "authors": [
        {
            "name": "Wesley Hott Soares",
            "email": "wesley.hott@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "orchestra/testbench": "^6.3",
        "spatie/laravel-permission": "^3.17",
        "subfission/cas":"^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Mpmg\\Laravue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpmg\\Laravue\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpmg\\Laravue\\LaravueServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
