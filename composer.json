{
    "name": "mpmg/laravue",
    "description": "Code generation for MPMG projects that use Laravel and VueJs tecnologies.",
    "keywords": [
        "mpmg",
        "vue",
        "vuejs",
        "laravel"
    ],
    "homepage": "https://github.com/mpmg-sti/laravue",
    "license": "MIT",
    "authors": [
        {
            "name": "Wesley Hott Soares",
            "email": "wesley.hott@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Mpmg\\Laravue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpmg\\Laravue\\Test": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpmg\\Laravue\\LaravueServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
